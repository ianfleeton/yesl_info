<h1><%= link_to h(@address.organisation.name), organisation_path(@address.organisation) %>
  &rarr; Edit address</h1>
<%= flash_notice %>

<div class="form">
  <%= error_messages_for :address %>
  <% form_for @address do |form| %>
    <%= render :partial => 'form', :object => form %>
    <%= submit_tag 'Save' %>
  <% end %>
</div>
