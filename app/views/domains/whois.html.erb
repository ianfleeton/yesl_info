<div class="page-header">
  <h1>whois <%= @domain.name %></h1>
</div>

<ul class="breadcrumb">
  <li>
    <%= link_to @domain.organisation.name, organisation_path(@domain.organisation) %>
    <span class="divider">/</span>
  </li>
  <li>
    <%= link_to @domain.name, domain_path(@domain) %>
    <span class="divider">/</span>
  </li>
  <li class="active">
    WHOIS
  </li>
</ul>

<p>
